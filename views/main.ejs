<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-LN+7fdVzj6u52u30Kp6M/trliBMCMKTyK833zpbD+pXdCLuTusPj697FH4R/5mcr" crossorigin="anonymous">
    <title>Document</title>
</head>
<body>
  <h1 id="saludo">Cargando usuario...</h1>

  <ul>
    <% data.forEach(function(msj) { %>
      <li><%= msj %></li>
    <% }); %>
  </ul>
  <ul id="chat"></ul>


  <input type="text" id="msj" autocomplete="off" placeholder="Su mensaje">

  <div>
    <form id="contacto">
      <input type="number" id="idContacto" placeholder="Numero de contacto">
      <button type="submit">Hablar</button>
    </form>
  </div>

  <button onclick="limpiar()">Limpiar Chat</button>

  <div>
    <form id = "salir">
      <button type="submit">Salir</button>
    </form>
  </div>



  <script src="/socket.io/socket.io.js"></script>
  <script src="/main.js"></script>

</body>
</html>